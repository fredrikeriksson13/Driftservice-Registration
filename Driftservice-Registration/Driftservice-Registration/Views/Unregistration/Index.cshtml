@model DriftService.Models.ContactViewModel
<link rel="stylesheet" href="~/Content/StyleSheet.css" />

@{
    ViewBag.Title = "Avsluta prenumeration";
}

<div class="container">
    <div class="row">
        <div class="text-center">
            <h1> Avsluta din prenumeration av Driftservice-Notifikation</h1>
        </div>
    </div>
    <div class="row">
        <br />
        <div class="text-center">
            @using (Html.BeginForm())
            {
                <input class="btn btn-default" type="button" value="Avprenumerera" onclick="location.href='@(Url.Action("Unregister", "Unregistration", new { guid = Model.ContactGuidAsString }) )'" />

            }
        </div>
    </div>

    <div class="row">
        <div class="text-center"><b>@ViewBag.Unregister</b></div>
    </div>
</div>












